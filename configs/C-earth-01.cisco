enable
configure terminal

ip routing

interface g0/0/0
  no shutdown

interface g0/0/0.10
  encapsulation dot1Q 10
  ip address 10.5.10.1 255.255.255.0

interface g0/0/0.11
  encapsulation dot1Q 11
  ip address 10.5.11.1 255.255.255.0

interface se0/1/0
  ip address 10.0.0.42 255.255.255.252
  ip ospf 10 area 0
  ip ospf cost 1000
  no shutdown
  exit

interface se0/1/1
  ip address 10.0.0.46 255.255.255.252
  ip ospf 10 area 0
  ip ospf cost 1000
  no shutdown
  exit

router ospf 10
  router-id 10.0.0.46
  network 10.5.10.0 0.0.0.255 area 0
  network 10.5.11.0 0.0.0.255 area 0
